{
  "name": "@bevy-inspector/vscode-extension",
  "main": "../../dist/vscode-extension/index.js",
  "scripts": {
    "prebuild": "npm run check-types",
    "build": "node esbuild.mjs --production",
    "build-tests": "tsc --noEmit false",
    "check-types": "tsc",
    "clean": "rimraf out ../../.vscode-test",
    "lint": "eslint src",
    "pretest": "npm run build-tests && npm run lint",
    "test": "vscode-test --config ../../.vscode-test.mjs",
    "watch": "node esbuild.mjs --watch"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^12.0.1",
    "axios": "^1.8.4",
    "fast-levenshtein": "^3.0.0",
    "json-rpc-types": "^0.1.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.5",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.30",
    "@types/vscode": "^1.99.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.24.2"
  }
}
