{
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "outDir": "out",
    "rootDir": "src",
    "allowImportingTsExtensions": false,
    "noEmit": true,
    "sourceMap": true,
    "declarationMap": true,
    "paths": {
      "@bevy-inspector/messenger/*": ["../messenger/out/*"],
      "@bevy-inspector/inspector-messages": ["../inspector-messages/out/index"]
    },
    "composite": true,
    "tsBuildInfoFile": "../../node_modules/.tmp/selection-view.tsbuildinfo"
  },
  "include": ["src"],
  "references": [
    {
      "path": "../messenger"
    },
    {
      "path": "../inspector-messages"
    }
  ]
}
